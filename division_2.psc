Algoritmo division_2
	Definir n1, n2, x, n Como Real;
	Escribir "INSERTA EL PRIMER NUMERO";
	leer n1;
	Escribir "INSERTA EL SEGUNDO NUMERO";
	leer n2;
	Si(n1>n2) Entonces
		x=n1/n2;
		Escribir "el resultado de division es: ", x;
	SiNo
		n=n2/n1;
		Escribir "el resultado de division es: ", n;
	FinSi
	
FinAlgoritmo
